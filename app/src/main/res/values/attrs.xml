<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyImageView">
        <attr name="hightweight" format="integer" />
        <attr name="widthtweight" format="integer" />
    </declare-styleable>

    <declare-styleable name="RongDivisionEditText">
        <attr name="totalLength" format="integer" />
        <attr name="sperator" format="string" />
        <attr name="type">
            <enum name="phone" value="1" />
            <enum name="bank" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PickerView">
        <attr name="mMaxTextSize" format="dimension" />
        <attr name="mMinTextSize" format="dimension" />
        <attr name="mMaxTextAlpha" format="float" />
        <attr name="mMinTextAlpha" format="float" />
        <attr name="mColorText" format="color" />
        <attr name="mLineText" format="color" />
        <attr name="mSpace" format="dimension" />
        <attr name="marginAlpha" format="float" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="lineSpacingMultiplier" format="float" />
        <attr name="itemsVisible" format="integer" />
        <attr name="velocityfling" format="integer" />
        <attr name="scalecontent" format="float" />
        <attr name="colorGray" format="color" />
        <attr name="colorBlack" format="color" />
        <attr name="colorLightGray" format="color" />
    </declare-styleable>

    <attr name="fontsize_1" format="dimension"></attr>

    <attr name="textColor" format="color|reference" />
    <attr name="mainBackground" format="color|reference" />
</resources>